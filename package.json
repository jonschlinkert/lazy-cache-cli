{
  "name": "lazy-cache-cli",
  "description": "Super basic CLI for adding a `utils.js` file for adding lazy-cache to a project.",
  "version": "0.1.1",
  "homepage": "https://github.com/jonschlinkert/lazy-cache-cli",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/lazy-cache-cli",
  "bugs": {
    "url": "https://github.com/jonschlinkert/lazy-cache-cli/issues"
