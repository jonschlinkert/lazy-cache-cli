{
  "name": "lazy-cache-cli",
  "description": "CLI for adding a `utils.js` file with lazy-cached requires.",
  "version": "0.2.1",
  "homepage": "https://github.com/jonschlinkert/lazy-cache-cli",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/lazy-cache-cli",
  "bugs": {
    "url": "https://github.com/jonschlinkert/lazy-cache-cli/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "lib",
    "utils.js"
  ],
  "preferGlobal": true,
  "bin": {
    "lazy": "bin/cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-app": "^0.2.3",
    "base-fs-conflicts": "^0.1.6",
    "base-store": "^0.4.4",
    "fs-exists-sync": "^0.1.0",
    "inject-snippet": "^0.3.0",
    "lazy-cache": "^2.0.1",
    "through2": "^2.0.1",
    "yargs-parser": "^2.4.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "cache",
    "lazy",
    "lazy-cache",
    "load",
    "module",
    "require",
    "requires"
  ],
  "lintDeps": {
    "ignore": [
      "engines.js"
    ]
  },
  "verb": {
    "related": {
      "list": [
        "lazy-cache",
        "lint-deps"
      ]
    },
    "reflinks": [
      "lazy-cache",
      "verb"
    ],
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
